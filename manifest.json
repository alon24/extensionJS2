{
  "manifest_version": 3,
  "name": "CRXJS React Vite Example",
  "version": "1.0.0",
  "action": { "default_popup": "popup.html" },
  "permissions": [ "activeTab" ],
  "content_scripts": [
    {
      "js": ["src/contentContainer.jsx"],
      "matches": ["https://www.google.com/*"]
    }
  ],
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  }
}